(define x (list 'a 'b))
(define z1 (cons x x))
;;       -----
;; z1--->|.|.|
;;       -----
;;        | |    -----    -----
;;    x--------->|.|.|--->|.|/|
;;               -----    -----
;;                |  ---   |  ---
;;                -->|a|   -->|b|
;;                   ---      ---

(define z2 (cons (list 'a 'b) (list 'a 'b)))
;;       -----
;; z2--->|.|.|
;;       -----
;;        | |    -----    -----
;;        | ---->|.|.|--->|.|/|
;;        |      -----    -----
;;        |       |  ---   |  ---
;;        |       -->|a|   -->|b|
;;        |       |  ---   |  ---
;;        |      -----    -----
;;        |----->|.|.|--->|.|/|
;;               -----    -----

(define (set-to-wow! x)
  (set-car! (car x) 'wow)
  x)

(set-to-wow! z1) ; => ((wow b) wow b)
(set-to-wow! z2) ; => ((wow b) a b)
