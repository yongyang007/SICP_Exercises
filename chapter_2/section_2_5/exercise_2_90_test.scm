(add-load-path "./")
(load "generic_arithmetic_system.scm")

(define p1 (make-sparse-polynomial 'x '((2 1) (1 2) (0 1))))
(define p2 (make-sparse-polynomial 'x '((3 2) (2 1) (1 1))))
(add p1 p2)
(mul p1 p2)
(sub p1 p1)
(sub p1 p2)
(define p3 (make-dense-polynomial 'x '(1 2 1)))
(define p4 (make-dense-polynomial 'x '(2 1 1 0)))
(add p3 p4)
(mul p3 p4)
(sub p3 p3)
(sub p3 p4)
